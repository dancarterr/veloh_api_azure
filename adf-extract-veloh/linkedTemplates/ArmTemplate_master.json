{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-extract-veloh"
		},
		"blob_veloh_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_veloh'"
		},
		"Veloh Rest API_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.jcdecaux.com/vls/v1/stations?apiKey=33a792c68888383c3eaa4b96a3c6678a798a9dd3"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-extract-veloh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blob_veloh_connectionString": {
						"value": "[parameters('blob_veloh_connectionString')]"
					},
					"Veloh Rest API_properties_typeProperties_url": {
						"value": "[parameters('Veloh Rest API_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}